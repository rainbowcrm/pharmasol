<?xml version="1.0" encoding="UTF-8"?>
<Page id="newAdhowAppointment" showinFrame="true" title="Adhoc Visit  | Primus WFM"
      Object="com.primus.crm.appointment.model.Appointment"
      Controller="com.primus.crm.appointment.controller.AppointmentController">
    <Template type="Transaction" name="TransactionTemplate"></Template>
    <Forwards>
        <Forward key="CANCEL">controller?page=appointments</Forward>
        <Forward key="SAVE">appointments</Forward>
    </Forwards>
    <IncludeJS>./js/CRMGeneral.js</IncludeJS>
    <IncludeJS>./js/RadsLib.js</IncludeJS>
    <AjaxGroups>
        <AjaxGroup service="regionLocationSearch">
            <requestElements>
                <requestElement key="region">LstRegion</requestElement>
            </requestElements>
            <responseElements>
                <responseElement key="location">LstLocations</responseElement>
            </responseElements>
        </AjaxGroup>
        <AjaxGroup service ="itemSearch">
            <requestElements event = "">
                <requestElement key = "Code">txtItemCode</requestElement>
            </requestElements>
            <responseElements>

                <responseElement key = "Code">txtItemCode</responseElement>
                <responseElement key = "Name">txtItemName</responseElement>

            </responseElements>
        </AjaxGroup>

        <AjaxGroup service ="skuSearch">
            <requestElements event = "">
                <requestElement key = "Code">txtSkuCode</requestElement>
            </requestElements>
            <responseElements>

                <responseElement key = "Code">txtSkuCode</responseElement>
                <responseElement key = "Name">txtSkuName</responseElement>

            </responseElements>
        </AjaxGroup>
        <AjaxGroup service ="skuSearch">
            <requestElements event = "">
                <requestElement key = "Code">txtOrdLineSkuCode</requestElement>
            </requestElements>
            <responseElements>

                <responseElement key = "Code">txtOrdLineSkuCode</responseElement>
                <responseElement key = "Name">txtOrdLineSkuName</responseElement>
                <responseElement key = "UOM.Code">txtOrdLineUOMCode</responseElement>
                <responseElement key = "RetailRate">txtOrdLineRate</responseElement>

            </responseElements>
        </AjaxGroup>
        <AjaxGroup service ="skuSearch">
            <requestElements event = "">
                <requestElement key = "Code">txtStkOrdLineSkuCode</requestElement>
            </requestElements>
            <responseElements>

                <responseElement key = "Code">txtStkOrdLineSkuCode</responseElement>
                <responseElement key = "Name">txtStkOrdLineSkuName</responseElement>
                <responseElement key = "UOM.Code">txtStkOrdLineUOMCode</responseElement>
                <responseElement key = "RetailRate">txtStkOrdLineRate</responseElement>

            </responseElements>
        </AjaxGroup>

    </AjaxGroups>
