<?xml version="1.0" encoding="UTF-8"?>
<Page id = "$pageID" showinFrame ="true" title ="$pageTitle | Primus WFM" Object = "$entityWithPackage"
      Controller = "$crudControllerwithPackage">
    <Template type= "CRUD"  name = "CRUDTemplate"></Template>
    <Forwards>
        <Forward key ="CANCEL">controller?page=$listPage</Forward>
        <Forward key ="SAVE">$listPage</Forward>
    </Forwards>
    <IncludeJS>./js/CRMGeneral.js</IncludeJS>
    <IncludeJS>./js/RadsLib.js</IncludeJS>
    <Elements>
        <UIForm Id ="cust" method = "POST" >
            <CoreSection>
                <ErrorSection>
                </ErrorSection>
                <Section type ="detail">
                    <Element label = "" type = "UIHidden" Id = "hdnVersion" property ="version" />
                    <Element label = ""  type = "UIHidden" Id = "hdnUserID"  property ="Id" />
                    #foreach ($field in $uiDisplayfields)
                    $field.uielementdef
                    #end
                </Section>
            </CoreSection>
            <Section type ="summary">
            </Section>

        </UIForm>
    </Elements>
</Page>